<%- include ("partials/header") -%>
<!-- Title for this page -->
<title>Ticket Collector Page</title>

<!-- User Page Style CSS -->
<link rel="stylesheet" href="css/user_style.css">


</head>


<body>

  <div class="wrap">

    <nav class="navbar navbar-expand-lg navbar-light ">
      <a id="logout" class="btn mr-0" href="/logout" role="button">Log Out</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
    </nav>

    <br>
    <div class="wrap">
      <div class="w-50 p-3" style="width: 800px; margin: 0 auto;">

        <form action="/scan_qr" method="POST">
          <label for="scan_qr">Enter QR Code:</label>
          <br><br>
          <input type="text" name="qr_code">

          <br><br>
          <button type="submit" class="btn btn-success">Scan</button>

        </form>
        <br>

        <label><%= label_fname %></label>
        <p><%= bookingFistName %></p>

        <label><%= label_lname %></label>
        <p><%= bookingLastName %></p>

        <label><%= label_phone %></label>
        <p><%= bookingPhone %></p>

        <img src="<%= bookingImage %>" class="rounded-circle"
        style="width: <%= width %>px; height: <%= height %>px;">

      </div>
    </div>

  </div>

  <div class="footer">
    <div>
      <a href="http://www.facebook.com" title="Facebook"><i class="fab fa-facebook-f footer-icon"></i></a>
      <a href="http://www.twitter.com" title="Twitter"><i class="fab fa-twitter footer-icon"></i></a>
      <a href="http://www.instagram.com" title="Instagram"><i class="fab fa-instagram footer-icon"></i></a>
      <a href="http://www.gmail.com" title="Email"><i class="fas fa-envelope footer-icon"></i></a>
      <p id="copyright">Â© Copyright 2021</p>
    </div>
  </div>

  <!-- Font Awesome Scripts -->
  <script src="https://kit.fontawesome.com/dfa9e54a69.js" crossorigin="anonymous"></script>
  <!-- Bootstrap section. -->
  <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.min.js" integrity="sha384-w1Q4orYjBQndcko6MimVbzY0tgp4pWB4lZ7lr30WKz0vr/aWKhXdBNmNb5D92v7s" crossorigin="anonymous"></script>
</body>

</html>
